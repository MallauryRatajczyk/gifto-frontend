{
  "expo": {
    "name": "gifto-frontend",
    "slug": "gifto-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoGifto.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoGiftoSpaced.png",
      "backgroundColor": "#F7FAFE",
      "resizeMode": "center"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoGiftoMinify.png",
        "backgroundColor": "#F7FAFE",
        "resizeMode": "contain"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoGiftoMinify.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "path/to/file.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}